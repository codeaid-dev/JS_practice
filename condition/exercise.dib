#!javascript

// break
let n = 0;
while (n < 3) {
  n++;
  console.log(`n:${n}`);
  if (n == 2) {
    break;
  }
}

#!javascript

// continue
let n = 0;
while (n < 3) {
  n++;
  if (n == 2) {
    continue;
  }
  console.log(`n:${n}`);
}

#!javascript

// 最初の文は "loop1" というラベルが付いている
loop1: for (let i = 0; i < 3; i++) {
  // 2 番目の文は "loop2" というラベルが付いている
  loop2: for (let j = 0; j < 3; j++) {
    if (i === 1 && j === 1) {
      continue loop1;
    }
    console.log(`i = ${i}, j = ${j}`);
  }
}


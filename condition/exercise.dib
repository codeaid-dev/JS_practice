#!javascript

// for ... in , for ... ofの違い
const arr = [3, 5, 7];
arr.foo = "hello";

for (let i in arr) {
  console.log(i); // "0", "1", "2", "foo"
}
console.log(arr.foo);
console.log(arr['foo']);
for (let i of arr) {
  console.log(i); // 3, 5, 7
}

